@{
    Layout = "~/Views/Shared/_DarkEdenLayout.cshtml";
}
<link rel="stylesheet" type="text/css" href="~/Styles/MarketStyle.css" />
<h1>Market Place</h1>
<div class="content">
    <div id="slayer">
        <a href="~/Market/Slayer"><img src="~/Images/Slayer_Banner.png"/></a>
        <div id="slayerItems">
            <div class="itemMarket">
               @Html.Partial("_SlayerShortPartial")
            </div>
        </div>
    </div>
    <div id="vampire">
        <a href="~/Market/Vampire"><img src="~/Images/Vampire_Banner.png" /></a>
        <div id="vampireItems">
            <div class="itemMarket">
                @Html.Partial("_VampireShortPartial")

            </div>
        </div>
    </div>
    <div id="ouster">
        <a href="~/Market/Ouster"><img src="~/Images/Ouster_Banner.png" /></a>
        <div id="ousterItems">
            <div class="itemMarket">
                @Html.Partial("_OusterShortPartial")

            </div>
        </div>

    </div>
    <div id="common">
        <a href="~/Market/Common"><img src="~/Images/Common_Banner.png" /></a>
        <div id="commonItems">
            <div class="itemMarket">
                @Html.Partial("_CommonShortPartial")

            </div>
        </div>
    </div>
</div>

<div class="checkout">
    @Html.Partial("_GoldPartial")
</div>
