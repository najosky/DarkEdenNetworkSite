
@{
    ViewBag.Title = "ContactUs";
    Layout = "~/Views/Shared/_DarkEdenLayout.cshtml";
}

<link rel="stylesheet" type="text/css" href="~/Styles/InfoStyle.css" />
<link rel="stylesheet" type="text/css" href="~/Styles/ContactUsStyle.css" />


<h1>Info</h1>
<div class="content">
    <ul>
        <li>@Html.ActionLink("Game Rules", "Info", "Home", new { @class = "SecondNav" })</li>
        <li>@Html.ActionLink("Server Rates", "ServerRates", "Home", new { @class = "SecondNav" })</li>
        <li>@Html.ActionLink("Contact Us", "ContactUs", "Home", new { @class = "SecondNav" })</li>
    </ul>

    <article>
        <section class="body-content">
            <h2>Contact Us</h2>
            <table>
                <tr>
                    <td>Server Owner:</td>
                    <td>GameName</td>
                    <td>Owner@darkedennetwork.com</td>
                </tr>
                <tr>
                    <td>Administrator:</td>
                    <td>GameName</td>
                    <td>Admin@darkedennetwork.com</td>
                </tr>
                <tr>
                    <td>Game-Master(GM):</td>
                    <td>[GM]GameName</td>
                    <td>GameMaster@darkedennetwork.com</td>
                </tr>
                <tr>
                    <td>Player-Manager(PM):</td>
                    <td>[PM]GameName</td>
                    <td>PlayerManager@darkedennetwork.com</td>
                </tr>
            </table>
        </section>
    </article>
</div>

